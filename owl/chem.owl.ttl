            owl:onProperty <https://w3id.org/chemschema/isotope_of> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty <https://w3id.org/chemschema/decay_energy> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <https://w3id.org/chemschema/Isotope> ;
            owl:onProperty <https://w3id.org/chemschema/decay_product> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass <https://w3id.org/chemschema/NumberOfYears> ;
            owl:onProperty <https://w3id.org/chemschema/half_life> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass linkml:String ;
            owl:onProperty <https://w3id.org/chemschema/decay_mode> ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
