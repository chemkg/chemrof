            owl:onClass linkml:String ;
            owl:onProperty <https://w3id.org/chemschema/smarts_string> ] ;
    skos:definition "reaction" .

<https://w3id.org/chemschema/MoleculeGroupingClass> a owl:Class,
        linkml:ClassDefinition ;
    rdfs:label "molecule grouping class" ;
    rdfs:subClassOf [ a owl:Restriction ;
