chemrof:Allotrope a sh:NodeShape ;
    sh:closed true ;
    sh:description "A molecule consisting of atoms of the same element" ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "allotrope" ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "The part of an InChI string that represents the chemical formula, occurring after the version string. E.g. C6H12O6 for glucose" ;
            sh:maxCount 1 ;
            sh:name "inchi chemical sublayer" ;
            sh:nodeKind sh:Literal ;
            sh:order 10 ;
            sh:path chemrof:inchi_chemical_sublayer ;
            sh:pattern "^[A-Z0-9\\.]+$" ],
        [ sh:datatype xsd:float ;
            sh:description "The sum of the relative atomic masses of the constituent atoms of a molecule." ;
            sh:maxCount 1 ;
            sh:name "molecular mass" ;
            sh:nodeKind sh:Literal ;
            sh:order 14 ;
            sh:path chemrof:molecular_mass ],
        [ sh:class owl:Class ;
            sh:description "Holds between C and P if C owl:subClassOf P. Note that in this model, most classes are metaclasses (see 'owl class'), where these metaclasses may represent *groupings* or specific countable elements or species. We provide two different sub-properties of subclass of for two cases: (1) subtype_of, which connects grouping classes (e.g. alkane is_a hydrocarbon; alpha-amino-acid is_a amino-acid), (2) classified_by, which connects a specific element to a grouping class (e.g. methane is_a hydrocarbon; iron is_a metal; serine is_a amino-acid)" ;
            sh:maxCount 1 ;
            sh:name "owl subclass of" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 21 ;
            sh:path chemrof:owl_subclass_of ],
        [ sh:class chemrof:ChemicalRole ;
            sh:maxCount 1 ;
            sh:name "has chemical role" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 8 ;
            sh:path chemrof:has_chemical_role ],
        [ sh:datatype xsd:string ;
            sh:description "name of chemical entity. E.g. nickel, carbon-16" ;
            sh:maxCount 1 ;
            sh:name "name" ;
            sh:nodeKind sh:Literal ;
            sh:order 23 ;
            sh:path rdfs:label ],
        [ sh:datatype xsd:float ;
            sh:description "The negative logarithm of the acid dissociation constant (Ka) for ionizable groups in a molecule. Multiple pKa values may exist for molecules with multiple ionizable groups." ;
            sh:name "pKa ionization constant" ;
            sh:nodeKind sh:Literal ;
            sh:order 16 ;
            sh:path chemrof:pka_ionization_constant ],
        [ sh:class chemrof:Molecule ;
            sh:description "Relation between a molecule and the molecules it is made from." ;
            sh:name "has submolecules" ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path chemrof:has_submolecules ],
        [ sh:datatype xsd:boolean ;
            sh:description "An organic molecule is a molecule composed of organic atoms (at least carbon, hydrogen, and optionally oxygen, phosphorus, nitrogen, sulfur) [SIO]" ;
            sh:maxCount 1 ;
            sh:name "is organic" ;
            sh:nodeKind sh:Literal ;
            sh:order 4 ;
            sh:path chemrof:is_organic ],
        [ sh:class chemrof:AtomOccurrence ;
            sh:description "An individuated atom in the context of a chemical structure." ;
            sh:minCount 2 ;
            sh:name "has atom occurrences" ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path chemrof:has_atom_occurrences ],
        [ sh:datatype xsd:string ;
            sh:description "Solvent system for pKa measurement. Default is water (H₂O) if not specified." ;
            sh:maxCount 1 ;
            sh:name "pKa solvent" ;
            sh:nodeKind sh:Literal ;
            sh:order 19 ;
            sh:path chemrof:pka_solvent ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "id" ;
            sh:nodeKind sh:Literal ;
            sh:order 22 ;
            sh:path chemrof:id ],
        [ sh:datatype xsd:float ;
            sh:description "Temperature in Celsius at which pKa was measured. Default is 25°C if not specified." ;
            sh:maxCount 1 ;
            sh:name "pKa temperature" ;
            sh:nodeKind sh:Literal ;
            sh:order 17 ;
            sh:path chemrof:pka_temperature ],
        [ sh:datatype xsd:float ;
            sh:description "Pressure in atmospheres at which pKa was measured. Default is 1 atm if not specified." ;
            sh:maxCount 1 ;
            sh:name "pKa pressure" ;
            sh:nodeKind sh:Literal ;
            sh:order 20 ;
            sh:path chemrof:pka_pressure ],
        [ sh:datatype xsd:float ;
            sh:description "The maximum amount of a substance that can dissolve in water at 25°C, expressed in grams per liter (g/L)." ;
            sh:maxCount 1 ;
            sh:name "water solubility" ;
            sh:nodeKind sh:Literal ;
            sh:order 15 ;
            sh:path chemrof:water_solubility ],
        [ sh:datatype xsd:float ;
            sh:description "Ionic strength of solution in mol/L at which pKa was measured. Default is 0.0 M if not specified." ;
            sh:maxCount 1 ;
            sh:name "pKa ionic strength" ;
            sh:nodeKind sh:Literal ;
            sh:order 18 ;
            sh:path chemrof:pka_ionic_strength ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:name "InChi" ;
            sh:nodeKind sh:Literal ;
            sh:order 9 ;
            sh:path chemrof:inchi_string ],
        [ sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
            sh:name "is radical" ;
            sh:nodeKind sh:Literal ;
            sh:order 7 ;
            sh:path chemrof:is_radical ],
        [ sh:datatype xsd:string ;
            sh:description "A chemical formula where numbers denote relative proportion of atoms. E.g. CH2O for glucose" ;
            sh:maxCount 1 ;
            sh:name "empirical formula" ;
            sh:nodeKind sh:Literal ;
            sh:order 12 ;
            sh:path chemrof:empirical_formula ],
        [ sh:datatype xsd:string ;
            sh:description "A string encoding of a molecular graph, no chiral or isotopic information. There are usually a large number of valid SMILES which represent a given structure. For example, CCO, OCC and C(O)C all specify the structure of ethanol." ;
            sh:maxCount 1 ;
            sh:name "smiles string" ;
            sh:nodeKind sh:Literal ;
            sh:order 11 ;
            sh:path chemrof:smiles_string ],
        [ sh:class chemrof:ChemicalEntity ;
            sh:description "Links different variants of the same chemical in different protonation states, where the target of this link is the (predicited) to be most common protonation state for this chemical at a pH of 7.3. This link may point to it's source. Connects an acid to the form that is stable at physiological pH (7.3). E.g. citric acid has physiological base citrate(3-)" ;
            sh:name "has physiologically stable form" ;
            sh:nodeKind sh:IRI ;
            sh:order 13 ;
            sh:path chemrof:has_major_microspecies_at_pH7_3 ],
        [ sh:datatype xsd:string ;
            sh:description "The atoms in the chemical formula (except for hydrogens) are numbered in sequence; this sublayer describes which atoms are connected by bonds to which other ones." ;
            sh:maxCount 1 ;
            sh:name "inchi atom connections sublayer" ;
            sh:nodeKind sh:Literal ;
            sh:order 5 ;
            sh:path chemrof:inchi_atom_connections_sublayer ;
            sh:pattern "^c.*" ],
        [ sh:class chemrof:Atom ;
            sh:description "Relation between a molecule and the atoms it contains. Note that this is a shortcut relation: for granular representations go via atom occurrences" ;
            sh:name "has atoms" ;
            sh:nodeKind sh:IRI ;
            sh:order 3 ;
            sh:path chemrof:has_atoms ],
        [ sh:class gc:Bond ;
            sh:description "The set of all bonds connecting atoms in a molecule" ;
            sh:name "has bonds" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:order 1 ;
            sh:path chemrof:has_bonds ],
        [ sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:name "IUPAC name" ;
            sh:nodeKind sh:Literal ;
            sh:order 6 ;
            sh:path CHEMINF:000107 ],
        [ sh:datatype xsd:anyURI ;
            sh:maxCount 1 ;
            sh:name "type" ;
            sh:nodeKind sh:Literal ;
            sh:order 24 ;
            sh:path chemrof:type ] ;
    sh:targetClass chemrof:Allotrope .

